deftype pair(int x, y) {}

function testme (pair p) : pair
{
  chp {
    self := p
  }
}

defproc test(chan?(int) X, Y; chan!(int) O) {
  pair p;

  chp {
    *[
      X?p.x,
      Y?p.y;
      p.x := testme(p).x;
      *[  p.x > p.y ->  p.x := p.x - p.y
      [] p.y > p.x ->  p.y := p.y - p.x
      ];
      O!p.x
    ]
  }
}

test t;
